                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginLeft="200dp"
                android:layout_below="@id/taxCountry"
                android:id="@+id/taxSameCountry"/>
            <TextView
                android:paddingTop="5dp"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tax another country*"
                android:textColor="@color/colorPrimary"
                android:layout_below="@+id/tsid"
                android:id="@+id/taid"/>
            <TextView
                android:paddingTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginLeft="200dp"
                android:layout_below="@id/taxSameCountry"
                android:id="@+id/taxAnotherCountry"/>
            <TextView
                android:paddingTop="5dp"
                android:paddingLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tin number*"
                android:textColor="@color/colorPrimary"
                android:layout_below="@+id/taid"
                android:id="@+id/tinid"/>
            <TextView
                android:paddingTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginLeft="200dp"
                android:layout_below="@id/taxAnotherCountry"
                android:id="@+id/tin_no"/>



